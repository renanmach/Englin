<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>Englin</display-name>
  <context-param>
    <param-name>javax.servlet.jsp.jstl.fmt.localizationContext</param-name>
    <param-value>i18n.Messages</param-value>
  </context-param>
  <context-param>
    <param-name>javax.servlet.jsp.jstl.fmt.fallbackLocale</param-name>
    <param-value>en</param-value>
  </context-param>
  <context-param>
    <param-name>javax.servlet.jsp.jstl.fmt.locale</param-name>
    <param-value>en</param-value>
  </context-param>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  	<filter>
		<description></description>
		<display-name>CharsetFilter</display-name>
		<filter-name>CharsetFilter</filter-name>
		<filter-class>filter.CharsetFilter</filter-class>
	</filter>
  
  <servlet>
	<description>RememberItStartServlet</description>
	<display-name>RememberItStartServlet</display-name>
	<servlet-name>RememberItStartServlet</servlet-name>
	<servlet-class>servlet.RememberItStartServlet</servlet-class>
  </servlet>
  <servlet>
	<description>RememberItActionServlet</description>
	<display-name>RememberItActionServlet</display-name>
	<servlet-name>RememberItActionServlet</servlet-name>
	<servlet-class>servlet.RememberItActionServlet</servlet-class>
  </servlet>
	
</web-app>